<?xml version="1.0" encoding="utf-8"?>



<shortcuts
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:targetApi="n_mr1">

    <shortcut
        android:shortcutId="downloads"
        android:icon="@mipmap/downloads_shortcut_icon"
        android:shortcutShortLabel="@string/shortcut_downloads_title">

        <intent
            android:action="com.baidu.duer.files.intent.action.VIEW_DOWNLOADS"
            android:targetPackage="com.baidu.duer.files" />
    </shortcut>

    <shortcut
        android:shortcutId="ftp_server"
        android:icon="@mipmap/ftp_server_shortcut_icon"
        android:shortcutShortLabel="@string/shortcut_ftp_server_title">

        <intent
            android:action="com.baidu.duer.files.intent.action.MANAGE_FTP_SERVER"
            android:targetPackage="com.baidu.duer.files" />
    </shortcut>
</shortcuts>
